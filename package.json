{
  "name": "react-redux-boilerplate",
  "version": "0.1.0",
  "description": "react-redux-boilerplate",
  "main": "src/index.js",
  "license": "MIT",
  "repository": "",
  "scripts": {
    "start": "npm run dll:dev && webpack-dashboard -- node server.js",
    "clean": "rimraf lib dist dll coverage",
    "lint": "eslint src scripts *.js",
    "testonly": "cross-env NODE_ENV=test jest",
    "testonly:watch": "npm run testonly -- --watch",
    "test": "npm run lint && npm run testonly",
    "test:cov": "npm run lint && npm run testonly -- --coverage",
    "check": "npm run lint && npm run test",
    "dll:dev": "cross-env NODE_ENV=development webpack --config webpack.config.dll.js",
    "dll:prd": "cross-env NODE_ENV=production webpack --config webpack.config.dll.js",
    "build": "npm run clean && npm run dll:prd && cross-env NODE_ENV=production webpack src/index.js --config webpack.config.prod.js && rimraf dll",
    "upload": "gulp upload-release"
  },
  "author": {
    "name": "Go7hic",
    "email": "gtfx0209@qq.com"
  },
  "devEngines": {
    "node": ">=4"
  },
  "keywords": [
    "redux",
    "react"
  ],
